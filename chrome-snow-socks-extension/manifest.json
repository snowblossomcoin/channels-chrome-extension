{
  "name": "Snowblossom Socks5 Enabler",
  "version": "1.0",
  "description": "Enables sock5s for access to channels",
  "icons": {
    "256": "icons/flower-on-256.png",
    "128": "icons/flower-on-128.png",
    "48":  "icons/flower-on-48.png",
    "16":  "icons/flower-on-16.png"
  },


  "permissions": ["storage","proxy"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "manifest_version": 2,
  "minimum_chrome_version":"22.0.0",

  "browser_action": {
    "default_icon": "icons/flower-off.png",
    "default_title": "Snowblossom Channels Proxy",
    "default_popup": "popup.html"
   }

}

